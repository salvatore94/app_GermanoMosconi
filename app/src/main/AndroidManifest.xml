<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.polito.salvatore.germanomosconi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/germano"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BiographyActivity"
            android:label="@string/title_activity_biography"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Nemici1Activity"
            android:label="@string/title_activity_nemici1"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Nomi1Activity"
            android:label="@string/title_activity_nomi1"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
